{
    "name": "Boilerplate Extension Webpack",
    "description": "Boilerplate - Extension, Webpack, React, Material Ui.",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.bundle.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.bundle.js"
            ]
        }
    ]
}